<?xml version="1.0"?>
 
<PropertyList>

 <fx>


  <crank>
   <name>engine1-start</name>
   <path>Aircraft/DavePack/Sounds/Censtart.wav</path>
   <property>engines/engine[0]/cranking</property>
   <volume>
    <offset>-0.4</offset>
   </volume>
  </crank>

  <crank>
   <name>engine2-start</name>
   <path>Aircraft/DavePack/Sounds/Censtart.wav</path>
   <property>engines/engine[1]/cranking</property>
   <volume>
    <offset>-0.4</offset>
   </volume>
  </crank>

  <crank>
   <name>engine3-start</name>
   <path>Aircraft/DavePack/Sounds/Censtart.wav</path>
   <property>engines/engine[2]/cranking</property>
   <volume>
    <offset>-0.4</offset>
   </volume>
  </crank>

  <crank>
   <name>engine4-start</name>
   <path>Aircraft/DavePack/Sounds/Censtart.wav</path>
   <property>engines/engine[3]/cranking</property>
   <volume>
    <offset>-0.4</offset>
   </volume>
  </crank>

  <engine>
   <name>engine1-rpm</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/radial-1000.wav</path>
   <condition>
    <property>engines/engine[0]/running</property>
    <greater-than>
      <property>engines/engine[0]/rpm</property>
      <value>300</value>
    </greater-than>
   </condition>
   <volume>
    <property>fdm/jsbsim/propulsion/engine[0]/map-inhg</property>
    <type>sqrt</type>
    <factor>0.125</factor>
   </volume>
   <pitch>
    <property>fdm/jsbsim/systems/engine-sound/pitch</property>
   </pitch>
  </engine>

  <engine>
   <name>engine2-rpm</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/radial-1000.wav</path>
   <condition>
    <property>engines/engine[1]/running</property>
    <greater-than>
      <property>engines/engine[1]/rpm</property>
      <value>300</value>
    </greater-than>
   </condition>
   <volume>
    <property>fdm/jsbsim/propulsion/engine[1]/map-inhg</property>
    <type>sqrt</type>
    <factor>0.125</factor>
   </volume>
   <pitch>
    <property>fdm/jsbsim/systems/engine-sound/pitch</property>
   </pitch>
  </engine>

  <engine>
   <name>engine3-rpm</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/radial-1000.wav</path>
   <condition>
    <property>engines/engine[2]/running</property>
    <greater-than>
      <property>engines/engine[2]/rpm</property>
      <value>300</value>
    </greater-than>
   </condition>
   <volume>
    <property>fdm/jsbsim/propulsion/engine[2]/map-inhg</property>
    <type>sqrt</type>
    <factor>0.125</factor>
   </volume>
   <pitch>
    <property>fdm/jsbsim/systems/engine-sound/pitch</property>
   </pitch>
  </engine>

  <engine>
   <name>engine4-rpm</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/radial-1000.wav</path>
   <condition>
    <property>engines/engine[3]/running</property>
    <greater-than>
      <property>engines/engine[3]/rpm</property>
      <value>300</value>
    </greater-than>
   </condition>
   <volume>
    <property>fdm/jsbsim/propulsion/engine[3]/map-inhg</property>
    <type>sqrt</type>
    <factor>0.125</factor>
   </volume>
   <pitch>
    <property>fdm/jsbsim/systems/engine-sound/pitch</property>
   </pitch>
  </engine>


