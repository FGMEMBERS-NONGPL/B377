  <!-- Engine shutdown -->
  <shutdown>
   <name>engine1-shutdown</name>
   <path>Aircraft/DavePack/Sounds/CentSdown_3.wav</path>
   <condition>
    <equals>
      <property>controls/engines/engine[0]/mixture</property>
      <value>0</value>
    </equals>
    <greater-than>
      <property>/engines/engine[0]/rpm</property>
      <value>10</value>
    </greater-than>
   </condition>
  </shutdown>
  <shutdown>
   <name>engine2-shutdown</name>
   <path>Aircraft/DavePack/Sounds/CentSdown_3.wav</path>
   <condition>
    <equals>
      <property>controls/engines/engine[1]/mixture</property>
      <value>0</value>
    </equals>
    <greater-than>
      <property>/engines/engine[1]/rpm</property>
      <value>10</value>
    </greater-than>
   </condition>
  </shutdown>
  <shutdown>
   <name>engine3-shutdown</name>
   <path>Aircraft/DavePack/Sounds/CentSdown_3.wav</path>
   <condition>
    <equals>
      <property>controls/engines/engine[2]/mixture</property>
      <value>0</value>
    </equals>
    <greater-than>
      <property>/engines/engine[2]/rpm</property>
      <value>10</value>
    </greater-than>
   </condition>
  </shutdown>
  <shutdown>
   <name>engine4-shutdown</name>
   <path>Aircraft/DavePack/Sounds/CentSdown_3.wav</path>
   <condition>
    <equals>
      <property>controls/engines/engine[3]/mixture</property>
      <value>0</value>
    </equals>
    <greater-than>
      <property>/engines/engine[3]/rpm</property>
      <value>10</value>
    </greater-than>
   </condition>
  </shutdown>

  <rumble>
   <name>rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <or>
     <property>gear/gear[0]/wow</property>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
    <property>sim/current-view/internal</property>
   </condition>
   <volume>
    <property>velocities/airspeed-kt</property>
    <type>log</type>
    <factor>0.1</factor>
   </volume>
  </rumble>

  <gear>
   <name>gear</name>
   <mode>in-transit</mode>
   <path>Aircraft/DavePack/Sounds/LMGEARUP.WAV</path>
   <property>gear/gear[0]/position-norm</property>
  </gear>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <or>
     <property>gear/gear[0]/wow</property>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>velocities/speed-down-fps</property>
    <factor>0.5</factor>
    <max>1.0</max>
   </volume>
   <volume>
    <property>velocities/airspeed-kt</property>
    <factor>0.01</factor>
    <max>1.0</max>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>0.5</offset>
    <max>1.0</max>
   </pitch>
  </squeal>

<!-- 
  <flap>
   <name>flaps</name>
   <mode>in-transit</mode>
   <path>Aircraft/A-1H/Sounds/CMFLAP.WAV</path>
   <property>surface-positions/flap-pos-norm</property>
   <volume>
    <factor>0.35</factor>
   </volume>
  </flap>
-->

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind.wav</path>
   <condition>
      <property>sim/current-view/internal</property>
   </condition>
   <property>velocities/airspeed-kt</property>
   <volume>
     <property>velocities/airspeed-kt</property>
     <factor>0.000375</factor>
     <min>0.00</min>
     <max>0.15</max>
   </volume>
   <pitch>
    <property>velocities/airspeed-kt</property>
    <factor>0.005</factor>
    <offset>0.5</offset>
    <max>1.0</max>
   </pitch>
  </wind>

  <buffet>
   <name>alpha-buffet</name>
   <mode>looped</mode>
   <condition>
    <greater-than>
     <property>fdm/jsbsim/aero/alpha-rad[0]</property>
     <value>0.2</value>
    </greater-than>
    <not>
     <property>gear/gear[1]/wow</property>    
    </not>
    <property>sim/current-view/internal</property>
   </condition>
   <path>Sounds/rumble.wav</path>
   <volume>
    <property>fdm/jsbsim/aero/alpha-rad</property>
    <factor>15.0</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>fdm/jsbsim/aero/alpha-rad</property>
    <factor>0.1</factor>
   </pitch>
  </buffet>


</fx>
 
</PropertyList>
